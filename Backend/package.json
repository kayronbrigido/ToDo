{
   "name": "todo",
   "version": "1.0.0",
   "scripts": {
      "dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
      "build": "tsc && node ./build/src/",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs -d ./src/configs/database.ts",
      "run:migration": "typeorm-ts-node-commonjs migration:run -d ./src/configs/database.ts",
      "createMigration": "cd ./src/migrations && typeorm-ts-node-commonjs migration:create"
   },
   "keywords": [
      "todo",
      "todo",
      "list"
   ],
   "author": "Kayron Brigido",
   "license": "ISC",
   "dependencies": {
      "body-parser": "^1.20.2",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "helmet": "^6.0.1",
      "http-status": "^1.6.2",
      "inversify": "^6.0.1",
      "inversify-express-utils": "^6.4.3",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.12",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/compression": "^1.7.2",
      "@types/cors": "^2.8.13",
      "@types/inversify-express-utils": "^2.0.0",
      "@types/node": "^16.18.23",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2"
   }
}
